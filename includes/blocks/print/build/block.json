{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "oes/print",
  "version": "0.1.0",
  "title": "Print Button",
  "category": "widgets",
  "description": "Display the print button.",
  "supports": {
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true
    },
    "align": true,
    "anchor": true,
    "alignContent": true,
    "color": {
      "text": true,
      "background": true,
      "link": true,
      "gradients": true
    },
    "alignText": true,
    "fullHeight": true
  },
  "attributes": {
    "icon": {
      "type": "boolean",
      "default": true
    },
    "labels": {
      "type": "object"
    }
  },
  "editorScript": "file:./index.js"
}