{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "oes/table-of-contents",
  "version": "0.1.0",
  "title": "Table of Contents",
  "category": "theme",
  "description": "Display the table of contents.",
  "keywords": [
    "structure",
    "navigation"
  ],
  "attributes": {
    "labels": {
      "type": "object"
    }
  },
  "supports": {
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true
    },
    "align": true,
    "anchor": true,
    "alignContent": true,
    "color": {
      "text": true,
      "background": true,
      "link": true
    },
    "alignText": true,
    "fullHeight": true
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}
