{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "oes/filter-index",
  "version": "0.1.0",
  "title": "Index Filter",
  "category": "oes-filter",
  "description": "Display the index filters.",
  "keywords": [
    "archive",
    "list",
    "navigation"
  ],
  "supports": {
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true
    },
    "align": true,
    "anchor": true,
    "alignContent": true,
    "color": {
      "text": true,
      "background": true,
      "link": true,
      "gradients": true
    },
    "alignText": true,
    "fullHeight": true
  },
  "attributes": {
    "include_all": {
      "type": "boolean",
      "default": true
    }
  },
  "styles": [
    {
      "name": "oes-vertical-list",
      "label": "Vertical",
      "isDefault": true
    },
    {
      "name": "oes-horizontal-list",
      "label": "Horizontal"
    }
  ],
  "editorScript": "file:./index.js",
  "render": "file:./render.php"
}