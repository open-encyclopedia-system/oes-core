{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "oes/terms",
  "version": "0.1.0",
  "title": "Terms",
  "category": "oes-schema",
  "description": "Display the OES terms of the post.",
  "keywords": [
    "keywords",
    "category"
  ],
  "attributes": {
    "detail": {
      "type": "boolean",
      "default": false
    },
    "labels": {
      "type": "object"
    }
  },
  "styles": [
    {
      "name": "oes-default",
      "label": "Default",
      "isDefault": true
    },
    {
      "name": "oes-box",
      "label": "Box"
    }
  ],
  "supports": {
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true
    },
    "align": true,
    "anchor": true,
    "alignContent": true,
    "color": {
      "text": true,
      "background": true,
      "link": true
    },
    "alignText": true,
    "fullHeight": true
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}