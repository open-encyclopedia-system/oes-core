{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "oes/literature",
  "version": "0.1.0",
  "title": "Literature List",
  "category": "oes-schema",
  "description": "Display the literature list of the OES post.",
  "keywords": [
    "literature",
    "bibliographic"
  ],
  "attributes": {
    "include_toc": {
      "type": "boolean",
      "default": true
    }
  },
  "supports": {
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true
    },
    "align": true,
    "anchor": true,
    "alignContent": true,
    "color": {
      "text": true,
      "background": true,
      "link": true
    },
    "alignText": true,
    "fullHeight": true
  },
  "editorScript": "file:./index.js",
  "render": "file:./render.php"
}
