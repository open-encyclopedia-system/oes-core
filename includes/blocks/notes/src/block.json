{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "oes/notes",
  "version": "0.1.0",
  "title": "Notes List",
  "category": "oes-schema",
  "description": "Display the OES notes list.",
  "keywords": [
    "end note",
    "foot note",
    "annotation"
  ],
  "attributes": {
    "include_toc": {
      "type": "boolean",
      "default": true
    },
    "labels": {
      "type": "object"
    }
  },
  "supports": {
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true
    },
    "align": true,
    "anchor": true,
    "alignContent": true,
    "color": {
      "text": true,
      "background": true,
      "link": true
    },
    "alignText": true,
    "fullHeight": true
  },
  "editorScript": "file:./index.js",
  "render": "file:./render.php",
  "style": "file:./style-index.css"
}
