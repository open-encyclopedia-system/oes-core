{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "oes/featured-image",
  "version": "0.1.0",
  "title": "Featured Image",
  "category": "oes-schema",
  "description": "Display the OES featured image of the post.",
  "keywords": [
    "picture",
    "jpg"
  ],
  "attributes": {
    "detail": {
      "type": "boolean",
      "default": false
    },
    "labels": {
      "type": "object"
    }
  },
  "supports": {
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true
    },
    "align": true,
    "anchor": true,
    "alignContent": true,
    "color": {
      "text": true,
      "background": true,
      "link": true
    },
    "alignText": true,
    "fullHeight": true
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}