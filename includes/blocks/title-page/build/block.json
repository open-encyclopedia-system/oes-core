{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "oes/title-page",
  "version": "0.1.0",
  "title": "Page Title",
  "category": "theme",
  "description": "Display the OES page title, e.g. as an archive link for posts.",
  "keywords": [
    "title",
    "navigation"
  ],
  "attributes": {
    "is_link": {
      "type": "boolean",
      "default": false
    }
  },
  "styles": [
    {
      "name": "default",
      "label": "Default",
      "isDefault": true
    },
    {
      "name": "uppercase",
      "label": "Uppercase"
    }
  ],
  "supports": {
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true
    },
    "align": true,
    "anchor": true,
    "alignContent": true,
    "color": {
      "text": true,
      "background": true,
      "link": true
    },
    "alignText": true,
    "fullHeight": true
  },
  "editorScript": "file:./index.js",
  "render": "file:./render.php"
}